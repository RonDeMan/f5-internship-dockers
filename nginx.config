server {
    listen 8080;
    server_name localhost;

    location / {
        add_header Content-Type text/html;
        return 200 '<html><body><h1>Hello from Port 8080!</h1></body></html>';
    }
}


server {
    listen 9090;
    server_name localhost;

    location / {
        return 403 'Access Denied';
    }
}